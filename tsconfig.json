{
  "compilerOptions": {
      "target": "es2022",
      "module": "node20",
      "moduleResolution": "node16",
      "strict": true,
      "esModuleInterop": true,
      "outDir": "./out",
      "rootDir": "./src",
      "pretty": true,
      "skipLibCheck": true,
      "noImplicitReturns": true,
      "noUncheckedIndexedAccess": false,
      "noUnusedLocals": true,
      "downlevelIteration": true
  }
}